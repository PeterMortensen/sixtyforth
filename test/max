#!/bin/sh
FORTH=${FORTH:-./64th}
while ! test -x "$FORTH" && [ "$(pwd)" != / ] ; do cd .. ; done

out=$($FORTH -c '-1 1 max .
-9223372036854775808 -9223372036854775807 max .
99 99 max .
9223372036854775806 9223372036854775807 max .
')
printf "%s\n" "$out"
case "$out" in
    ("1 -9223372036854775807 99 9223372036854775807 ") exit 0;;
esac
exit 2
