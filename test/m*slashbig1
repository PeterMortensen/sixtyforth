#!/bin/sh
FORTH=${FORTH:-./64th}
while ! test -x "$FORTH" && [ "$(pwd)" != / ] ; do cd .. ; done
# Product of a 47-bit number and two 40-bit numbers
out=$($FORTH -c '123000111000222 0 1000111000222 1 M*/
      1000111000222 1 M*/
      D.')
printf "%s\n" "$out"
case "$out" in
    ("123027418594966528145366302411582941048 ") exit 0;;
esac
exit 2
